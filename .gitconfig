[alias]
    ci = commit
    st = status
    co = checkout
    br = branch
    solve = mergetools -t kiff3
    patch = !"git ci -am 'f' & git rebase HEAD~2"
    reb = !"git co trunk && git pull && git co - && git rebase trunk"
